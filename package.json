{
  "name": "artconomy",
  "version": "3.1.0",
  "type": "module",
  "private": true,
  "description": "Where artists and commissioners meet!",
  "author": "Fox Piacenti <fox@vulpinity.com>",
  "scripts": {
    "serve": "vite --host",
    "build:wasm": "wasm-pack build --dev --out-dir=../../frontend/lib/lines rust/line_items --features=wasm",
    "build": "wasm-pack build --release --out-dir=../../frontend/lib/lines rust/line_items --features=wasm && vue-tsc --noEmit && vite build",
    "build:quick": "vue-tsc --noEmit && vite build",
    "supportedBrowsers": "echo \"export default $(browserslist-useragent-regexp --allowHigherVersions);\" > frontend/supportedBrowsers.js",
    "lint": "eslint",
    "lint:fix": "eslint . --fix && prettier frontend --write",
    "test": "TZ=\"America/Chicago\" vitest"
  },
  "dependencies": {
    "@date-io/date-fns": "^3.2.1",
    "@devindex/vue-mask": "^2.0.3",
    "@mdi/js": "^7.4.47",
    "@sentry/vite-plugin": "^3.2.2",
    "@sentry/vue": "^9.5.0",
    "@stripe/stripe-js": "^5.10.0",
    "@tato30/vue-pdf": "^1.11.3",
    "@uppy/core": "^4.4.2",
    "@uppy/dashboard": "^4.3.2",
    "@uppy/url": "^4.2.2",
    "@uppy/xhr-upload": "^4.3.3",
    "axios": "^1.11.0",
    "browserslist": "*",
    "date-fns": "^4.1.0",
    "list-diff.js": "^0.1.0",
    "lodash.debounce": "^4.0.8",
    "markdown-it": "^14.1.0",
    "qrcode": "^1.5.4",
    "reconnecting-websocket": "^4.4.0",
    "roboto-fontface": "*",
    "simple-icons": "^14.10.0",
    "sortablejs": "^1.15.6",
    "sortablejs-vue3": "^1.2.11",
    "uuid": "^11.1.0",
    "vue": "^3.5.13",
    "vue-observe-visibility": "^2.0.0-alpha.1",
    "vue-router": "^4.5.0",
    "vuetify": "^3.9.7",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
